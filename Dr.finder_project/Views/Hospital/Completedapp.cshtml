@{
    ViewBag.Title = "Completedapp";
    Layout = "~/Views/Shared/Hospital_master_Layout.cshtml";
}


<link href="~/Dr_finder_tmp/Admin_penal/web/css/jquery.dataTables.min.css" rel="stylesheet" />
<script src="~/Dr_finder_tmp/Admin_penal/web/js/jquery.dataTables.min.js"></script>

<script>
        $(document).ready(function () {
            $('#example').DataTable({
                "pagingType": "full_numbers"
            });
        });
</script>

<div class="container-fluid">
    <div class="modal-header">

        <h2>Completed appointment</h2>
    </div>
    <div class="jumbotron">
        
        <div class="table-responsive">
            <table class="table table-striped" id="example">
                <thead>
                    <tr>
                        <th>
                            Doctor Info
                        </th>
                        <th>
                            User name
                        </th>
                        <th>
                            Booking Date
                        </th>
                        <th>
                            Appointment Date
                        </th>
                        <th>
                            Appointment Description
                        </th>
                    </tr>
                </thead>
                <tbody>
                    @foreach (var item in ViewData["happ"] as List<Dr.finder_project.Controllers.HospitalCmpDetails>)
                    {
                        <tr>
                            <td>
                                <img src="~/Images/@item.Doctor_Image" alt="" style="height:100px;width:100px;"/> <br />
                                @item.Doctor_fname  &nbsp; @item.Doctor_mname &nbsp;@item.Doctor_lname
                            </td>
                            <td>
                                @item.User_S_fNam  &nbsp; @item.User_S_mName &nbsp;@item.User_s_lName
                            </td>
                            <td>
                                @item.Appointment_Current_Date
                            </td>
                            <td>
                                @item.Appointment_Date
                            </td>
                            <td>
                                @item.Appointment_Description
                    </td>
                </tr>
                    }
                </tbody>
            </table>
        </div>
        
    </div>
    <div class="modal-footer">

    </div>
</div>